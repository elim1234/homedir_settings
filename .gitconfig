	name = jdschwartz
	email = jdschwartz@appliedminds.com
[color]
	branch = auto
	diff = auto
	status = auto
	ui = auto
[push]
	default = tracking
[alias]
  st = status
  ci = commit
  co = checkout
  br = branch
  lc = log ORIG_HEAD.. --stat --no-merges
  k  = !gitk --all
  di = diff -b
  smu = submodule update --init --recursive

